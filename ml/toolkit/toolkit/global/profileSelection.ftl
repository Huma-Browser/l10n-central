profile-selection-window =
    .title = { -brand-short-name } - ഉപയോക്തൃ പ്രൊഫൈല്‍ തെരഞ്ഞെടുക്കുക

profile-selection-button-accept =
    .label = { -brand-short-name } തുടങ്ങുക

profile-selection-button-cancel =
    .label = പുറത്തു കടക്കുക

profile-selection-new-button =
    .label = Create Profile...
    .accesskey = C

profile-selection-rename-button =
    .label = Profile പുനര്‍നാമകരണം...
    .accesskey = r

profile-selection-delete-button =
    .label = പ്രൊഫൈല്‍ മായിക്കുക
    .accesskey = D

## Messages used in the profile manager

profile-manager-description = { -brand-short-name }-ല്‍ നിങ്ങളുടെ ക്രമികരണങ്ങള്‍, മുന്‍ഗണനകള്‍, നിങ്ങളുടെ യൂസര്‍ പ്രൊഫൈലിലുള്ള മറ്റ് വസ്തുക്കള്‍ എന്നിവ അടങ്ങുന്നു.

profile-manager-work-offline =
    .label = ഓഫ്‌ലൈന്‍ ആയി പ്രവര്‍ത്തിക്കുക
    .accesskey = o

profile-manager-use-selected =
    .label = ആരംഭത്തില്‍ ചോദിയ്ക്കാതെ തെരഞ്ഞെടുത്ത പ്രൊഫൈല്‍ ഉപയോഗിയ്ക്കുക
    .accesskey = ഉ
