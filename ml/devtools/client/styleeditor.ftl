styleeditor-new-button =
    .tooltiptext = രേഖയിലേക്കു് പുതിയൊരു സ്റ്റൈല്‍ ഷീറ്റ് തയ്യാറാക്കി ചേര്‍ക്കുക
    .accesskey = N
styleeditor-import-button =
    .tooltiptext = രേഖയിലേക്കു് നിലവിലുള്ളൊരു സ്റ്റൈല്‍ ഷീറ്റ് ഇംപോര്‍ട്ട് ചെയ്തു് ചേര്‍ക്കുക
    .accesskey = I
styleeditor-visibility-toggle =
    .tooltiptext = സ്റ്റൈല്‍ ഷീറ്റ് കാഴ്ച മാറ്റുക
    .accesskey = S
styleeditor-save-button = സൂക്ഷിയ്ക്കുക
    .tooltiptext = ഒരു ഫയലിലേക്കു് ഈ സ്റ്റൈല്‍ ഷീറ്റ് സൂക്ഷിയ്ക്കുക
    .accesskey = S
styleeditor-options-button =
    .tooltiptext = സ്റ്റൈല്‍ എഡിറ്റര്‍ ഐച്ഛികങ്ങള്‍
styleeditor-editor-textbox =
    .data-placeholder = ഇവിടെ സിഎസ്എസ് ടൈപ്പ് ചെയ്യുക.
styleeditor-no-stylesheet = ഈ താളിനു് സ്റ്റൈല്‍ ഷീറ്റ് ലഭ്യമല്ല.
styleeditor-no-stylesheet-tip = നിങ്ങള്‍ക്കു് ഒരു പക്ഷേ <a data-l10n-name="append-new-stylesheet">ഒരു പുതിയ സ്റ്റൈല്‍ ഷീറ്റ് ചേര്‍ക്കണമോ</a>?
styleeditor-open-link-new-tab =
    .label = ലിങ്ക്പുതിയ റ്റാബില്‍ തുറക്കുക
styleeditor-find =
    .label = കണ്ടെത്തുക
    .accesskey = F
styleeditor-find-again =
    .label = വീണ്ടും കണ്ടെത്തുക
    .accesskey = g
styleeditor-go-to-line =
    .label = ഈ വരിയിലേക്കു് പോകുക…
    .accesskey = J

# This string is shown in the style sheets list
# Variables:
#   $ruleCount (Integer) - The number of rules in the stylesheet.
styleeditor-stylesheet-rule-count =
    { $ruleCount ->
        [one] { $ruleCount } നിയമം.
       *[other] { $ruleCount } നിയമങ്ങള്‍.
    }
