restore-page-tab-title = D'Sessioun restauréieren

# The title is intended to be apologetic and disarming, expressing dismay
# and regret that we are unable to restore the session for the user
restore-page-error-title = Et deet eis Leed, mir hu Schwieregkeeten Deng Säiten zeréck ze kréien.
restore-page-problem-desc = Mir hu Schwieregkeeten Deng lescht Sessioun ze restauréieren. Wiel Sessioun restauréieren fir nach emol ze probéieren.
restore-page-try-this = D'Sessioun nach ëmmer net restauréiert? Et ass méiglech dass en Tab Problemer mécht. Kuck d'Tab vu virdrun un, an huel den Hoken ewech vun deenen déi's Du net brauchs, a restauréier duerno frësch.

restore-page-hide-tabs = Tabs vu virdrun verstoppen
restore-page-show-tabs = Tabs vu virdrun uweisen

# When tabs are distributed across multiple windows, this message is used as a
# header above the group of tabs for each window.
#
# Variables:
#    $windowNumber: Progressive number associated to each window
restore-page-window-label = Fënster { $windowNumber }

restore-page-restore-header =
    .label = Restauréieren

restore-page-list-header =
    .label = Fënsteren an Tabs

restore-page-try-again-button =
    .label = Sessioun restauréieren
    .accesskey = R

restore-page-close-button =
    .label = Nei Sessioun starten
    .accesskey = N

