profile-selection-window =
    .title = { -brand-short-name } - वापरुप्याले प्रोफाईल वापरात

profile-selection-button-accept =
    .label = { -brand-short-name } सुरवात करात.

profile-selection-button-cancel =
    .label = भायर वचात

profile-selection-new-button =
    .label = प्रोफायल तयार करून…
    .accesskey = C

profile-selection-rename-button =
    .label = प्रोफायल नाव बदल…
    .accesskey = R

profile-selection-delete-button =
    .label = प्रोफायल काडून उडयात…
    .accesskey = D

## Messages used in the profile manager

profile-manager-description = तुमच्या वापरपी प्रोफायलान { -brand-short-name } तुमच्या स्थिपातांविशीं, प्राधान्यांविशीं आणि हेर वापरपी वस्तींविशीं माहिती साठयता.

profile-manager-work-offline =
    .label = ऑफलायन काम करात
    .accesskey = क

profile-manager-use-selected =
    .label = सुरवेक विचारीनास्तना निवडिल्ले प्रोफायल वापरात
    .accesskey = व
